extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-dark-grey
                h3 Tese Recebida
            .w3-container
                table.w3-table-all
                    tr
                        th Nome
                        td= tese.nome
                    tr
                        th Número / Identificador
                        td= tese.numero
                    tr
                        th Curso
                        td= tese.curso
                    tr
                        th Instituição
                        td= tese.insti

                    tr
                        th Titulo
                        td= tese.titulo
                    tr
                        th Data
                        td= tese.data

                    tr
                        th Tipo
                        td= tese.tipo

            footer.w3-container.w3-dark-grey
                a(href ='http://localhost:4006/') 
                    | Voltar
                address Gerado por formApp
