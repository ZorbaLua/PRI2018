extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-dark-grey
                h3 Lista de Teses
            .w3-container
                br
                table.w3-table-all
                    tr
                        th Nome
                        th Número
                        th Curso
                        th Instituição

                        th Titulo
                        th Data
                        th Tipo
                    each al in lista
                        tr
                            td= al.nome
                            td= al.numero
                            td= al.curso
                            td= al.insti

                            td= al.titulo
                            td= al.data
                            td= al.tipo
                br
            footer.w3-container.w3-dark-grey
                a(href ='http://localhost:4006/') 
                    | Voltar 
                address Gerado por formApp
